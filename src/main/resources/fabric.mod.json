{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "A highly configurable Minecraft-Discord chat linking mod",
  "authors": [
    "potatoboy9999", "yitzy299"
  ],
  "contact": {
    "homepage": "https://github.com/QuiltServerTools/BlockBot",
    "sources": "https://github.com/QuiltServerTools/BlockBot",
    "issues": "https://github.com/QuiltServerTools/BlockBot/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/blockbot-discord/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      {
        "value": "io.github.quiltservertools.blockbotdiscord.BlockBotDiscord::INSTANCE"
      }
    ]
  },
  "mixins": [
    "blockbot-discord.mixins.json"
  ],
  "depends": {
    "java": ">=16",
    "minecraft": ">=1.19.0",
    "blockbot-api": ">=1.0.0",
    "fabric-language-kotlin": ">=1.7.0+kotlin.1.6.0",
    "fabric": ">=${fabricApi}",
    "fabricloader": ">=${fabricLoader}"
  }
}
